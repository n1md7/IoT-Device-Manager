{
  "name": "manager",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "type": "commonjs",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:cov": "cross-env NODE_ENV=test vitest run --coverage",
    "test:debug": "cross-env NODE_ENV=test vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "cross-env NODE_ENV=e2e vitest run --config ./vitest.config.e2e.mts",
    "gen:env-types": "npx gen-env-types .env.example -o env.d.ts -k -e .",
    "gen:docs": "npx @compodoc/compodoc -p tsconfig.doc.json --output public",
    "typeorm": "cross-env NODE_ENV=development typeorm-ts-node-commonjs --dataSource src/common/setup/config/migration.config.ts",
    "migrations": "npm run typeorm -- migration:run",
    "migration:create": "cross-env NODE_ENV=development typeorm-ts-node-esm migration:create",
    "premigration:generate": "npm run build",
    "migration:generate": "npm run typeorm -- migration:generate --pretty",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show"
  },
  "engines": {
    "node": ">=21.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/microservices": "^10.2.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cron": "^3.1.6",
    "cron-parser": "^4.9.0",
    "cross-env": "^7.0.3",
    "js-yaml": "^4.1.0",
    "mqtt": "^5.3.1",
    "ms": "^2.1.3",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.10",
    "@types/js-yaml": "^4.0.9",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "unplugin-swc": "^1.4.3",
    "vitest": "^0.34.6"
  }
}
